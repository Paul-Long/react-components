{
  "name": "react-components",
  "version": "1.0.0",
  "description": "react-components",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run start",
    "deploy": "better-npm-run deploy",
    "start:prod": "better-npm-run start:prod"
  },
  "betterScripts": {
    "start": {
      "command": "node index",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "deploy": {
      "command": "webpack --config ./webpack.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start:prod": {
      "command": "node index.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Paul-Long/react-components.git"
  },
  "keywords": [
    "react",
    "components"
  ],
  "author": "Paul",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Paul-Long/react-components/issues"
  },
  "homepage": "https://github.com/Paul-Long/react-components#readme",
  "dependencies": {
    "fast-table": "^1.2.3",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "add-dom-event-listener": "^1.0.2",
